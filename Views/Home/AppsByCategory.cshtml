@model List<ViewModels.ApplicationVM>

<head>
    <title>Apps by Category</title>
</head>

<div class="text-center">
    <h1>Apps in Category: @ViewBag.SelectedCategoryName</h1>
    <div> </div>
    @if (Model.Count == 0)
    {
        <p>No apps found for this category.</p>
    }
    else
    {      
        <ul>
            @foreach (var app in Model)
            {
                <li>@app.Name</li>
            }
        </ul>
    }
</div>




